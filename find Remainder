#include<stdio.h>
int main()
{
    int a,b; // a>b
    printf("Enter the value of divident:");
    scanf("%d",&a);
    printf("Enter the value of divisor:");
    scanf("%d",&b);
    int q=a/b;
    int r= a-b*q; // remainder = divident - divisor * quotient
    printf("The remainder when %d is divided by %d is : %d",a,b,r);
    return 0;
}
