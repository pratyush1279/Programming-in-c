#include<stdio.h>
int main(){
    int cp;// ek dabba bna cost price ka 
    printf("Enter cost price : ");
    scanf("%d",&cp);
    int sp;// ek dabba bna selling price ka 
    printf("Enter selling price : ");
    scanf("%d",&sp);
    if(cp>sp){
        printf("It is loss");
    }
    if(sp>cp){
        printf("It is profit");
    }
    if(cp==sp){
        printf("No profit, No loss");
    }
    return 0;
}
