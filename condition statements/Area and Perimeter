#include<stdio.h>
int main(){
    int l;
    printf("Enter length of rectange : ");
    scanf("%d",&l);
    int b;
    printf("Enter breadth of rectange : ");
    scanf("%d",&b);
    int a = l*b;
    int p = 2 * (l+b);
    if(a>p){
        printf("Area is greater than perimeter");
    }
    if(p>a){
        printf("Area is smaller than perimeter");
    }
    if(p==a){
        printf("Area is equal to perimeter");
    }
    return 0;
}
